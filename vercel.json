{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "htmls/**/*",
                    "css/**/*",
                    "routers/**/*",
                    "imgs/**/*",
                    "js/**/*"
                ]
            }
        }
    ],
    "env": {
        "ROOT_DIR": "."
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
